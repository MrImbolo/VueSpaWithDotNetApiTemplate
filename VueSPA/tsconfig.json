{
	"compilerOptions": {
		//"rootDir": "src/",
		//"outDir": "src/",
		//"allowJs": true,
		"declaration": true,
		"forceConsistentCasingInFileNames": true,
		"target": "ESNext",
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Node"
	},
	"include": [
		"src/lib/*.ts",
		"src/plugins/*.ts"
	],
	"exclude": [
		"node_modules",
		"./node_modules",
		"./node_modules/*",
		"./node_modules/@types/node/index.d.ts"
	]
}